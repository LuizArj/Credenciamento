# ===========================================
# EXEMPLO DE VARIÁVEIS DE AMBIENTE
# ===========================================
# Copie este arquivo para .env.local e preencha com os valores reais
# NUNCA commite arquivos .env com valores reais!

# ===========================================
# APLICAÇÃO
# ===========================================
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# ===========================================
# SUPABASE
# ===========================================
NEXT_PUBLIC_SUPABASE_URL=https://seu-projeto.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=sua-chave-anon-key-aqui
SUPABASE_SERVICE_ROLE_KEY=sua-service-role-key-aqui
SUPABASE_SERVICE_KEY=sua-service-key-aqui

# ===========================================
# AUTENTICAÇÃO - KEYCLOAK
# ===========================================
KEYCLOAK_CLIENT_ID=seu-client-id
KEYCLOAK_CLIENT_SECRET=seu-client-secret
KEYCLOAK_ISSUER=https://seu-keycloak.com/auth/realms/seu-realm
KEYCLOAK_REALM=seu-realm

# ===========================================
# AUTENTICAÇÃO - NEXTAUTH
# ===========================================
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=sua-secret-key-aleatoria-aqui-min-32-chars

# ===========================================
# API EXTERNA - CPE (SEBRAE)
# ===========================================
CPE_CLIENT_ID=seu-cpe-client-id
CPE_CLIENT_SECRET=seu-cpe-client-secret
CPE_API_URL=https://api.cpe.sebrae.com.br

# ===========================================
# API EXTERNA - SAS (SEBRAE)
# ===========================================
SEBRAE_API_KEY=sua-api-key-sas
SEBRAE_COD_UF=24
SAS_API_URL=https://api.sas.sebrae.com.br
SAS_API_KEY=sua-api-key-sas

# ===========================================
# API EXTERNA - 4EVENTS
# ===========================================
FOUR_EVENTS_CLIENT_TOKEN=seu-token-4events
FOUR_EVENTS_API_URL=https://api.4events.com.br

# ===========================================
# WEBHOOKS
# ===========================================
N8N_WEBHOOK_URL=https://n8nhook.rr.sebrae.com.br/webhook/Credenciamento_checkin_sistema
N8N_WEBHOOK_CHECKIN_URL=https://seu-n8n.com/webhook/checkin
NEXT_PUBLIC_WEBHOOK_URL=https://n8nhook.rr.sebrae.com.br/webhook/Credenciamento_checkin_sistema

# ===========================================
# SEGURANÇA
# ===========================================
# Chave de acesso para validação de requisições
NEXT_PUBLIC_ACCESS_KEY=your_access_key_here

# URL de fallback para redirecionamentos
NEXT_PUBLIC_FALLBACK_URL=http://localhost:3000

# ===========================================
# LOGS E MONITORAMENTO
# ===========================================
LOG_LEVEL=info
ENABLE_LOGGING=true

# ===========================================
# RATE LIMITING
# ===========================================
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000
